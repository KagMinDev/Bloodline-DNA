{
  "compilerOptions": {
    "target": "esnext",
    "module": "Node16",
    "lib": ["dom", "esnext"],
    "jsx": "react-native",
    "moduleResolution": "node16",
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["expo"]
  },
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
